// using insert function with a vector whose size is not declared

#include <iostream>
#include<vector>
#include<algorithm>

using namespace std;

int main()
{
    
   vector<int> vec;
  vec.emplace_back(1);
  vec.insert(vec.begin(), 100);
  
    for(auto i: vec){
        cout << i <<" ";
    }

    return 0;
}

O/p: [100 1]

------------------------------------------------------------
// using insert function with a vector whose size is declared

#include <iostream>
#include<vector>
#include<algorithm>

using namespace std;

int main()
{
    
  vector<int> vec(5);
//   vector<int> vec;

  vec.emplace_back(1);
  
  vec.insert(vec.begin(), 100);
  
  
    for(auto i: vec){
        cout << i <<" ";
    }

    return 0;
}

o/p : [100 0 0 0 0 0 1]

---------------------------------------------------------------------------------------------------------------------------
//v.insert(v.begin+1, value);

#include <iostream>
#include<vector>
#include<algorithm>

using namespace std;

int main()
{
    
//   vector<int> vec(5);  // o/p:  [100 200 0 0 0 0 0 1]
    vector<int> vec;        // o/p:  [100 200 1]

  vec.emplace_back(1);
  
  vec.insert(vec.begin(), 100);
  vec.insert(vec.begin()+1, 200);
  
  
    for(auto i: vec){
        cout << i <<" ";
    }

    return 0;
}
-----------------------------------------------------------------------------------------------------

(v.insert(v.begin()+1, 3, 60) : [100 60 60 60 0 0 0 0 0 1]


