#include <iostream>
#include <algorithm>
#include <set>
#include <map>
using namespace std;
void explainMap(){
    map<int,int>mp;
    mp[1]=2;
    mp.emplace(make_pair(3,1));
    mp.insert({2,4});
    
    for(auto &it: mp){
        cout<< it.first<<" "<<it.second<<endl;
    }
    
    
    
    
    
//     // storing single value at pair of keys - declaration-3
