//nested pairs:

// pairs are part of utility library
// this is initialisation method
#include<iostream>
#include<vector>
#include<utility>
using namespace std;
    void expVec(){
        pair<int, pair<int,int>> p={2,{3,5}};
        cout<< p.first << " "<< p.second.first<<" "<<p.second.second<<endl;
        
        pair<int, pair<string,char>> p1={3201,{"sharon",'A'}};
        // cout<< p1.first << " "<< p1.second.first<<" "<<p1.second.second<<endl;
        cout<< "roll no: "<< p1.first <<endl;
        cout<< "name: "<<p1.second.first <<endl;
        cout<<"section: "<<p1.second.second<<endl;



    }
int main(){
    expVec();
return 0;
}
