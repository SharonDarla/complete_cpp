#include<iostream>
#include<functional>
#include<algorithm>
using namespace std;
int main(){
  int arr[]= {3,89,1,45,2,67};
  // sort(arr,arr+5); //sortn this array from 1-5 ele
  // sort(arr,arr+6,greater<int>());//sorts in descending order
  sort(arr,arr+6,less<int>()); //sort in ascending order
  for(int i=0;i<6;i++){
    cout<<arr[i]<<endl;
  }
  return 0;
}
